{
  "name": "unide",
  "version": "0.2.1",
  "description": "Universal visual designer for Vaadin and Vaadin Components",
  "repository": "https://github.com/mjvesa/unide-src",
  "publisher": "mjvesa",
  "main": "public/extension.js",
  "bin": "./cli.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public",
    "test": "jest"
  },
  "author": "Matti Vesa",
  "license": "MIT",
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onCommand:extension.unide"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.unide",
        "title": "Unide"
      }
    ]
  },
  "dependencies": {
    "@vaadin/vaadin-core": "^14.0.3",
    "@vaadin/vaadin-grid": "^5.4.8",
    "@vaadin/vaadin-tabs": "^3.0.4",
    "codemirror": "^5.44.0",
    "electron": "^6.0.9",
    "file-saver": "^2.0.0-rc.4",
    "jszip": "^3.1.5",
    "jszip-utils": "^0.1.0",
    "prettier": "^1.16.4",
    "vanilla-picker": "^2.7.2"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.16.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "serve": "^11.0.2",
    "@types/vscode": "^1.32.0"
  }
}
